{
    "rewrites": [
      { "source": "/galary", "destination": "/photogalary.html" }
    ],
    "redirects": [
      { "source": "/old-page", "destination": "/new-page", "permanent": true }
    ],
    "cleanUrls": true,
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "max-age=0, must-revalidate" }
        ]
      }
    ]
  }